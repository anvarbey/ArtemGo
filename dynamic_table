CREATE TABLE `asar_db_dynamic`.`popup_dynamic` 
( `id` INT NOT NULL AUTO_INCREMENT , 
`h1` TEXT NULL DEFAULT NULL , 
`h2` TEXT NULL DEFAULT NULL ,
`h3` TEXT NULL DEFAULT NULL , 
`h4` TEXT NULL DEFAULT NULL ,
`h5` TEXT NULL DEFAULT NULL , 
`p1` TEXT NULL DEFAULT NULL , 
`p2` TEXT NULL DEFAULT NULL , 
`p3` TEXT NULL DEFAULT NULL , 
`img1` BLOB NULL DEFAULT NULL , 
`img2` BLOB NULL DEFAULT NULL , 
PRIMARY KEY (`id`)) ENGINE = InnoDB;
